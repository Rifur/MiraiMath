<Serializer>
	<Array>
		<Open cas="{" tex="\lbrace " />
		<Close cas="}" tex="\rbrace " />
		<Field cas="," />
		<Row cas=";" />
	</Array>

	<Matrix>
		<Open cas="[" tex="\begin{bmatrix} " />
		<Close cas="]" tex=" \end{bmatrix}" />
		<Field cas="," tex=" &amp; " />
		<Row cas=";" tex=" \\ " />
	</Matrix>

